hostname: haproxy-00
ssh_authorized_keys:
  - ssh-rsa  yan@ubuntu-yma
  - ssh-rsa  alexandregarcia@mbpdealexandre.bouyguesbox.fr
  - ssh-dss  agarcia@norsys-debian-agarcia
  - ssh-rsa  luya@new-host.home
write_files:
  - path: /etc/ssh/sshd_config
    permissions: 0600
    owner: root:root
    content: |
      # Use most defaults for sshd configuration.
      UsePrivilegeSeparation sandbox
      Subsystem sftp internal-sftp

      PermitRootLogin no
      AllowUsers core
      PasswordAuthentication no
      ChallengeResponseAuthentication no
  - path: /home/core/haproxy.cfg
    permissions: 0600
    owner: core:core
    content: |
      listen stats :5000
        mode http
        stats enable
        stats hide-version
        stats realm Haproxy\ Statistics
        stats uri /
        stats auth stats:stats



        52.28.111.34
        52.28.78.157
